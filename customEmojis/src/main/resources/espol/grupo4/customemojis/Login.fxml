<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="espol.grupo4.customemojis.LoginController">
    <children>
        <HBox alignment="CENTER" layoutX="80.0" layoutY="38.0" prefHeight="36.0" prefWidth="200.0" spacing="10.0">
            <Label text="Usuario:" />
            <TextField fx:id="usernameField" />
        </HBox>
        <HBox alignment="CENTER" layoutX="64.0" layoutY="84.0" prefHeight="36.0" prefWidth="200.0" spacing="10.0">
            <Label text="Contraseña:" />
            <PasswordField fx:id="passwordField" />
        </HBox>
        <Button fx:id="loginButton" layoutX="115.0" layoutY="135.0" mnemonicParsing="false" onAction="#handleLoginButtonAction" text="Iniciar sesión" />
        <Label fx:id="statusLabel" layoutX="110.0" layoutY="181.0" prefHeight="17.0" prefWidth="200.0" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    
</AnchorPane>
